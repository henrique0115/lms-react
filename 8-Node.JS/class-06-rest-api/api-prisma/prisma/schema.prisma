generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Users {
  id          Int       @id @default(autoincrement())
  name        String
  age         Int
  cpf         String    @unique
  created_at  DateTime  @default(now())
  updated_at  DateTime  @default(now()) @updatedAt
}