generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  id         Int      @id @default(autoincrement())
  name       String
  age        Int
  cpf        String   @unique(map: "Users_cpf_key")
  created_at DateTime @default(now())
  updated_at DateTime @default(now()) @updatedAt
}
